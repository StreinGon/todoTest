{
    "extends": "tslint-config-airbnb",
    "rules": {
        "no-console": false,
        "variable-name": false,
        "no-consecutive-blank-lines": [
            true,
            2
        ],
        "max-classes-per-file": false,
        "no-unused-variable": false,
        "prefer-array-literal": false,
        "max-line-length": [
            true,
            120
        ]
    }
}